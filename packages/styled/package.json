{
  "name": "@slimr/styled",
  "version": "2.1.34",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "private": false,
  "sideEffects": false,
  "keywords": [
    "chakra",
    "chakra-ui",
    "emotion",
    "styled-components",
    "preact",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/slimr.git"
  },
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esm": "OUT=esm; tsc -d -m es2020 --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "build:cjs": "OUT=cjs; tsc -d -m commonjs --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "clean": "rm -rf esm cjs",
    "prepack": "run-s build"
  },
  "peerDependencies": {
    "@types/react": "^16.8  || ^17 || ^18",
    "@types/react-dom": "^16.8  || ^17 || ^18",
    "react": "^16.8  || ^17 || ^18",
    "react-dom": "^16.8  || ^17 || ^18"
  },
  "dependencies": {
    "@slimr/css": "2.1.28",
    "@slimr/util": "3.2.25"
  }
}
