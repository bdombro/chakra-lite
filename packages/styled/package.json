{
  "name": "@slimr/styled",
  "version": "2.0.7",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "private": false,
  "sideEffects": false,
  "keywords": [
    "chakra",
    "chakra-ui",
    "emotion",
    "styled-components",
    "preact",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/slimr.git"
  },
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    }
  },
  "scripts": {
    "build": "npm-run-all clean --parallel build:*",
    "build:esm": "OUT=esm; tsc -d -m es2020 --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "build:cjs": "OUT=cjs; tsc -d -m commonjs --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "clean": "rm -rf esm cjs",
    "lint": "eslint .",
    "prepack": "run-s lint build && npm version patch"
  },
  "peerDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@slimr/css": "^2.0.8"
  }
}
