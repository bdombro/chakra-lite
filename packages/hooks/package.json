{
  "name": "@slimr/hooks",
  "version": "2.1.19",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "private": false,
  "sideEffects": false,
  "keywords": [
    "preact",
    "react",
    "swr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/slimr.git"
  },
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esm": "OUT=esm; tsc -d -m es2020 --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "build:cjs": "OUT=cjs; tsc -d -m commonjs --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "clean": "rm -rf esm cjs",
    "prepack": "run-s build"
  },
  "dependencies": {
    "@slimr/forms": "2.1.21",
    "@slimr/swr": "2.1.14",
    "@slimr/util": "2.2.9",
    "react-use": "^17.4.0"
  },
  "optionalDependencies": {
    "react": "^16.8  || ^17 || ^18",
    "react-dom": "^16.8  || ^17 || ^18"
  }
}
