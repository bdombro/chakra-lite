{
  "name": "@ustyle/props-react",
  "version": "1.0.6",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "private": false,
  "keywords": [
    "chakra",
    "chakra-ui",
    "emotion",
    "styled-components",
    "preact",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/ustyle.git"
  },
  "type": "module",
  "main": "./src/index.cjs",
  "module": "./src/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.cjs",
      "types": "./src/index.ts"
    },
    "./base": {
      "import": "./src/withStyleProps.js",
      "require": "./src/withStyleProps.cjs",
      "types": "./src/withStyleProps.ts"
    }
  },
  "files": [
    "src",
    "shared",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "find -E ./src -regex '.*(\\.js|\\.cjs|\\.d\\.ts|\\.map)' -delete",
    "prepack": "npm run build && npm version patch",
    "postpublish": "npm run clean"
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  }
}
