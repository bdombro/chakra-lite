{
  "name": "@slimr/router",
  "version": "1.0.3",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "private": false,
  "keywords": [
    "react-router",
    "preact",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/slimr.git"
  },
  "type": "module",
  "main": "./src/index.cjs",
  "module": "./src/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.cjs",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "find -E ./src -regex '.*(\\.js|\\.cjs|\\.d\\.ts|\\.map)' -delete",
    "prepack": "npm run build && npm version patch",
    "postpublish": "npm run clean"
  },
  "peerDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18"
  }
}
