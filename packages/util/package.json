{
  "name": "@slimr/util",
  "version": "2.2.5",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "private": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    },
    "./code-highlight": {
      "require": "./cjs/code-highlight.js",
      "import": "./esm/code-highlight.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/slimr.git"
  },
  "type": "module",
  "scripts": {
    "build": "run-p build:*",
    "build:esm": "OUT=esm; tsc -d -m es2020 --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "build:cjs": "OUT=cjs; tsc -d -m commonjs --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "clean": "rm -rf esm cjs",
    "prepack": "run-s build"
  },
  "dependencies": {
    "deep-object-diff": "1.1.9",
    "fast-deep-equal": "3.1.3",
    "fast-shallow-equal": "1.0.0",
    "highlight.js": "11.7.0",
    "is-what": "4.1.8",
    "merge-anything": "5.1.4",
    "nanoid": "4.0.1"
  }
}
