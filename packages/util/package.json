{
  "name": "@slimr/util",
  "version": "1.0.38",
  "author": "Brian Dombrowski",
  "license": "ISC",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    },
    "./code-highlight": {
      "require": "./cjs/code-highlight.js",
      "import": "./esm/code-highlight.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bdombro/slimr.git"
  },
  "type": "module",
  "scripts": {
    "build": "npm-run-all clean --parallel build:*",
    "build:esm": "OUT=esm; tsc -d -m es2020 --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "build:cjs": "OUT=cjs; tsc -d -m commonjs --outDir $OUT --noEmit false && cp -r src/* $OUT",
    "clean": "rm -rf esm cjs",
    "lint": "eslint .",
    "prepack": "run-s lint build"
  },
  "dependencies": {
    "deep-object-diff": "^1.1.5",
    "fast-deep-equal": "^3.1.3",
    "fast-shallow-equal": "^1.0.0",
    "highlight.js": "^11.7.0",
    "is-what": "^4.1.8",
    "merge-anything": "^5.1.4",
    "nanoid": "^4.0.1"
  }
}
